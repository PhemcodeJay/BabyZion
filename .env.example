
# BABYZION MARKET - Environment Variables
# Copy this to .env and fill in your credentials

# Flask Configuration
FLASK_ENV=production
PORT=5000

# PayPal Credentials (Get from https://developer.paypal.com/)
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_BASE_URL=https://api-m.sandbox.paypal.com

# CJ Dropshipping (Optional - for product sync)
CJ_EMAIL=your_cj_email_here
CJ_API_KEY=your_cj_api_key_here

# Production Settings
# For production, change PAYPAL_BASE_URL to:
# PAYPAL_BASE_URL=https://api-m.paypal.com
